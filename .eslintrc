{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "generators": true,
    "objectLiteralShorthandProperties": true,
    "templateStrings": true
  },
  "env": {
    "node": true
  },
  "rules": {
    "camelcase": [0],
    "comma-dangle": [0],
    "consistent-this": [1, "self"],
    "default-case": [2],
    "guard-for-in": [2],
    "indent": [2, 2],
    "max-nested-callbacks": [1, 3],
    "no-nested-ternary": [2],
    "no-self-compare": [2],
    "no-sync": [1],
    "no-underscore-dangle": [0],
    "no-throw-literal": [2],
    "no-void": [2],
    "radix": [2],
    "strict": [2, "global"],
    "vars-on-top": [2]
  },
  "settings": {
    "ecmascript": 6
  }
}
